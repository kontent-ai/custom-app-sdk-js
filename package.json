{
  "name": "@kontent-ai/custom-app-sdk",
  "version": "2.1.0",
  "description": "The Kontent.ai Custom App SDK enhances the integration of your custom app with the Kontent.ai platform.",
  "license": "MIT",
  "author": "Kontent.ai",
  "keywords": [
    "Kontent.ai",
    "Kontent AI",
    "Kontent Custom App SDK JS"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
